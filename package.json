{
  "version": "1.0.0",
  "license": "MIT",
  "name": "puppeteer-smart-cluster",
  "author": {
    "email": "freshcrowd@gmail.com",
    "name": "Denis Orlov",
    "url": "https://github.com/Cr0WD"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Cr0WD/puppeteer-smart-cluster"
  },
  "keywords": [
    "puppeteer",
    "chrome",
    "headless",
    "automation",
    "puppeteer-cluster"
  ],
  "module": "dist/puppeteer-smart-cluster.esm.js",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "lint": "tsdx lint",
    "prepare": "install-peers && tsdx build",
    "postinstall": "manypkg fix && manypkg check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5",
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "endOfLine": "lf",
    "useTabs": true,
    "tabWidth": 4
  },
  "publishConfig": {
    "access": "public",
    "branches": [
      "main"
    ]
  },
  "peerDependencies": {
    "puppeteer": "^24.37.2"
  },
  "devDependencies": {
    "@eslint/js": "10.0.1",
    "@manypkg/cli": "0.25.1",
    "@typescript-eslint/eslint-plugin": "8.54.0",
    "@typescript-eslint/parser": "8.54.0",
    "eslint": "10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-promise": "7.2.1",
    "eslint-plugin-security": "3.0.1",
    "eslint-plugin-unicorn": "62.0.0",
    "install-peers-cli": "^2.2.0",
    "prettier": "3.8.1",
    "puppeteer": "^24.37.2",
    "tsdx": "^2.0.0",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.54.0"
  }
}
